<%= title [t(:label_license_plural), licenses_path], t(:label_license_new) %>

<%= labelled_form_for @license do |f| %>
<%= render :partial => 'form', :locals => { :f => f} %>
<p>
  <%= f.submit t(:button_create) %>
  <%= f.submit t(:button_create_and_continue), :name => 'continue' %>
</p>
<% end %>
